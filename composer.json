{
    "name": "spaethtech/common",
    "description": "A library of common classes and functions.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "composer/composer": "^2.4"
    },
    "require-dev": {
        "ext-mbstring": "*",

        "phpunit/phpunit": "~9",
        "twig/twig": "^2.11"
    },
    "autoload": {
        "files": [
            "inc/globals.inc.php"
        ],
        "psr-4": {
            "SpaethTech\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaethTech\\": [
                "src/",
                "tests/",
                "examples/"
            ]
        }
    },
    "bin": [
        "bin/phpdoc-md"
    ]



}
